{
  "name": "ssr-playground",
  "version": "1.0.0",
  "scripts": {
    "webpack:page": "webpack --config webpack.page.js --watch",
    "webpack:server": "webpack --config webpack.server.js --watch",
    "webpack:start": "nodemon --watch build --exec node ./build/server.bundle.js",
    "dev": "npm-run-all --parallel webpack:*",
    "build": "webpack --config webpack.page.js && webpack --config webpack.server.js"
  },
  "dependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
